{
  "manifest_version": 3,
  "name": "lilimiter",
  "description": "Base Level Extension",
  "version": "1.0",
  "icons": {
    "16": "images/chess_16.png",
    "32": "images/chess_32.png",
    "128": "images/chess_128.png"
  },
  "content_scripts": [
    {
      "js": ["src/index.js"],
      "matches": ["https://lichess.org/*"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["storage"],

  "action": {
    "default_popup": "src/popup.html"
  }
}
